{
  "product_key": "cloud-services",
  "product_name": "云服务",
  "product_name_en": "Cloud Services",
  "category": "compute",
  "azure_url": "https://www.azure.cn/pricing/details/cloud-services/",
  
  "file_mapping": {
    "html_file": "data/prod-html/compute/cloud-services.html",
    "current_html_file": "data/current_prod_html/compute/cloud-services.html"
  },
  
  "extraction_config": {
    "complexity": "complex",
    "page_type": "complex_multi_filter",
    "has_filters": true,
    "has_tabs": true,
    "has_regions": true,
    "filter_types": ["region", "software", "category"],
    "expected_content_groups": 6,
    "priority_features": [
      "Title",
      "BannerContent", 
      "DescriptionContent",
      "PricingTables",
      "QaContent"
    ],
    "content_selectors": {
      "main_container": ".technical-azure-selector",
      "pricing_sections": ".pricing-page-section",
      "filter_containers": [".dropdown-container.software-kind-container", ".dropdown-container.region-container"],
      "tab_container": ".os-tab-nav.category-tabs",
      "content_groups": ".tab-panel[id^='tabContent']"
    }
  },
  
  "region_filter_config": {
    "enabled": true,
    "filter_type": "complex_multi_region",
    "regions": [
      {"key": "china-north", "display": "中国北部"},
      {"key": "china-north-2", "display": "中国北部 2"}, 
      {"key": "china-north-3", "display": "中国北部 3"},
      {"key": "china-east", "display": "中国东部"},
      {"key": "china-east-2", "display": "中国东部 2"}
    ],
    "default_region": "china-north-3"
  },
  
  "output_config": {
    "formats": ["json", "flexible", "html", "rag"],
    "default_format": "flexible",
    "quality_threshold": 0.85,
    "include_validation": true,
    "enable_metadata": true
  },
  
  "validation_rules": {
    "required_sections": ["Title", "BannerContent", "DescriptionContent"],
    "min_content_length": 100,
    "max_processing_time": 60,
    "content_quality_checks": ["html_structure", "text_content", "filter_mapping"]
  },
  
  "strategy_overrides": {
    "force_strategy": "complex",
    "enable_debug": false,
    "custom_processors": [],
    "fallback_strategy": "simple_static"
  }
}